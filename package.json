{
  "name": "react-ts-todoapp",
  "version": "1.0.0",
  "description": "Todo list app",
  "main": "index.js",
  "scripts": {
    "start": "node public/app.js",
    "server": "nodemon src/app.ts",
    "build": "tsc -p",
    "client": "npm start --prefix react-client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Jibril Aidarous",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.10.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.2",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
